  </div>
  <br>
  <div class="form-group">
    <label for="description">Description</label>
    <textarea name="description" id="description" class="form-control"><%= note.description %></textarea>
  </div>
  <br>
  <div class="form-group">
    <label for="subject">Subject</label>
    <textarea  name="subject" id="subject" class="form-control"><%= note.subject %></textarea>
  </div>
  <br>
  <div class="form-group">
    <label for="semester">Semester</label>
    <textarea  name="semester" id="semester" class="form-control"><%= note.semester %></textarea>
  </div>
