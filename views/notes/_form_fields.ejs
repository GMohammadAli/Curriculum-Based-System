<div class="form-group">
  <label for="title">Title</label>
  <input
    value="<%= note.title %>"
    type="text"
    name="title"
    id="title"
    class="form-control"
  />
</div>
<br />
<div class="form-group">
  <label for="description">Description</label>
  <textarea name="description" id="description" class="form-control">
<%= note.description %></textarea
  >
</div>
<br />
<div class="form-group">
  <label for="subject">Subject</label>
  <textarea name="subject" id="subject" class="form-control">
<%= note.subject %></textarea
  >
</div>
<br />
<div class="form-group">
  <label for="semester">Semester</label>
  <textarea name="semester" id="semester" class="form-control">
<%= note.semester %></textarea
  >
</div>
<br />
<div class="form-group">
  <label for="upload">Add link to Notes:</label>
  <input type="url" name="url" id="upload" value="<%= note.url %>" />
</div>
<br />
<a href="/notes" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>
